<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.uga.cs.finalproject_sharedshoppinglist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinalProject_SharedShoppingList">
        <activity
            android:name=".PurchasedItemsListActivity"
            android:exported="false" />
        <activity
            android:name=".PurchasedItemActivity"
            android:exported="false"
            android:parentActivityName=".HomeScreenActivity" />
        <activity
            android:name=".NewNeededItemActivity"
            android:exported="false"
            android:parentActivityName=".HomeScreenActivity" />
        <activity
            android:name=".NeededItemsActivity"
            android:exported="false"
            android:parentActivityName=".HomeScreenActivity" />
        <activity
            android:name=".HomeScreenActivity"
            android:exported="false" />
        <activity
            android:name=".RegisterUserActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>